{
  "manifest_version": 3,
  "name": "Arona.ai Translator(中文化)",
  "version": "0.0.2",
  "description": "把Arona.ai翻譯成中文",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://arona.ai/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dictionary.json",
        "students_mapping.json",
        "Event.json",
        "Club.json",
        "School.json",
        "CharacterSSRNew.json",
        "otherstu.json",
        "FamilyName_mapping.json",
        "Hobby_mapping.json",
        "skill_name_mapping.json",
        "skill_Desc_mapping.json",
        "furniture_name_mapping.json",
        "furniture_Desc_mapping.json",
        "item_name_mapping.json",
        "item_Desc_mapping.json",
        "equipment_Desc_mapping.json",
        "equipment_name_mapping.json",
        "stages_name_mapping.json",
        "stages_Event_mapping.json",
        "fonts/PerfectDOSVGA437.ttf",
        "fonts/U-OTF-ShinMGoUpr-DeBold.otf",
        "fonts/U-OTF-ShinMGoUpr-Medium.otf"
      ],
      "matches": ["https://arona.ai/*"]
    }
  ]
}
